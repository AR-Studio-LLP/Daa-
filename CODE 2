abc=['aryan','homies','manas','arav','nikku']
print(abc[1:3])
print(abc[:3]) #printing from first to 3
print(abc[3:])#printing after 3 to last 
xyz=['george','moulik']
combine=[abc,xyz]
print(combine)
#sorting
abc=['aryan','homies','manas','arav','nikku']
abc.sort()
print(abc)
efg=[1,3,4,5,6,55,35,2,4,56]
efg.sort()
print(efg)
#sort in reverse
efg=[1,3,4,5,6,55,35,2,4,56]
efg.sort(reverse=True)
print(efg)
